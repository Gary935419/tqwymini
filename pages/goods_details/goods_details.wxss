@import "../../wxParse/wxParse.wxss";

.pages {
  background: #f2f5f7;
}

.pro_img {
  background: #fff;
  padding: 40rpx 0;
  text-align: center;
}

/* .pro_img image {
  width: 400rpx;
  height: 400rpx;
} */

.pro_message {
  padding: 0 20rpx 25rpx;
  background: #fff;
  margin-bottom: 20rpx;
}

.pro_name {
  padding: 30rpx 0;
}

.details_name {
  font-size: 34rpx;
  color: #333;
  font-weight: bold;
}

.share {
  float: right;
  font-size: 20rpx;
  color: #8c8c8c;
  margin-top: 7rpx;
  border: none;
  background: #fff;
  border-left: 1rpx solid #e1e4e6;
  border-radius: 0;
}

.share_btn {
  width: auto;
  font-size: 24rpx;
  color: #8c8c8c;
  border: none;
  background: none;
  padding: 0;
  padding-top: 20rpx;
}

.share_btn::after, .share_btn::before {
  border: none;
}

.share_content {
  width: 100%;
  height: 300rpx;
  display: flex;
  padding: 20rpx 200rpx;
  justify-content: space-between;
  box-sizing: border-box;
}

.content_main {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.color_white {
  background: #fff !important;
}

.share_image {
  width: 100rpx;
  border-radius: 50rpx;
  border: 1rpx solid #ccc;
  height: 100rpx;
  overflow: hidden;
  background: #3cb034;
  display: flex;
  justify-content: center;
  align-items: center;
}

.share_image image {
  width: 70rpx !important;
  height: 70rpx !important;
}

.share::after, .share::before {
  border: none;
}

.share_icon {
  display: inline-block;
  width: 28rpx;
  height: 36rpx;
  background: url('data:image/png;base64,') no-repeat center;
  vertical-align: middle;
  background-size: 28rpx 36rpx;
  margin-right: 15rpx;
  margin-left: 30rpx;
}

.pro_price {
  font-size: 48rpx;
  color: #e60012;
}

.pro_price text {
  font-size: 24rpx;
}

.pro_describe {
  font-size: 24rpx;
  margin-top: 30rpx;
}

.kucun, .mpq {
  display: inline-block;
  color: #595959;
}

.mpq {
  text-align: right;
}

.kucun text {
  color: #999;
  display: inline-block;
  margin-right: 40rpx;
}

.mpq text {
  color: #999;
  display: inline-block;
  margin-right: 10rpx;
}

.select_parameter {
  padding: 0 20rpx 30rpx;
  background: #fff;
  margin-bottom: 30rpx;
}

.select_content {
  padding: 25rpx 0;
  border-bottom: 1rpx solid #edf0f2;
}

.select_content {
  font-size: 28rpx;
  color: #595959;
}

.select_content text {
  display: inline-block;
  margin-left: 25rpx;
  font-weight: bold;
  color: #333;
}

.select_icon {
  /* display: inline-block; */
  width: 23rpx;
  height: 12rpx;
  background: url('data:image/png;base64,') no-repeat center;
  background-size: 23rpx 12rpx;
  vertical-align: middle;
  float: right;
  margin-top: 15rpx;
}

.parameter {
  padding: 30rpx 0;
}

/* .parameter_li{
  margin-top: 30rpx;
} */

.label {
  display: inline-block;
  font-size: 24rpx;
  color: #8c8c8c;
  width: 58rpx;
}

.parameter_details {
  display: inline-block;
  margin-left: 40rpx;
  vertical-align: top;
  width: 86%;
}

.parameter_details text {
  display: inline-block;
  padding: 15rpx 40rpx;
  background: #f2f5f7;
  border-radius: 12rpx;
  font-size: 26rpx;
  color: #595959;
  margin-right: 30rpx;
  margin-bottom: 30rpx;
}

.parameter_details .parameter_act {
  background: #0068c6;
  color: #fff;
}

.parameter_details .left_icon {
  display: inline-block;
  margin: 0;
  border-top-right-radius: 0;
  border-bottom-right-radius: 0;
  padding: 0;
  width: 56rpx;
  height: 56rpx;
  background: url('data:image/png;base64,') no-repeat center;
  background-size: 28rpx 4rpx;
  vertical-align: middle;
  border: 1px solid #e1e4e6;
}

.num_inp {
  outline: none;
  border: 1px solid #e1e4e6;
  display: inline-block;
  width: 118rpx;
  text-align: center;
  font-size: 28rpx;
  color: #595959;
  height: 56rpx;
  line-break: 56rpx;
  vertical-align: middle;
  border-left: none;
  border-right: none;
}

.parameter_details .right_icon {
  display: inline-block;
  margin: 0;
  border-top-left-radius: 0;
  border-bottom-left-radius: 0;
  padding: 0;
  width: 56rpx;
  height: 56rpx;
  border: 1px solid #e1e4e6;
  vertical-align: middle;
  background: url('data:image/png;base64,') no-repeat center;
  background-size: 28rpx 28rpx;
}

.pro_content {
  padding: 0 20rpx 92px;
  background: #fff;
}

.pro_title {
  font-size: 28rpx;
  color: #595959;
  padding: 25rpx 0;
  border-bottom: 1rpx solid #edf0f2;
}

.pro_container {
  text-align: center;
  padding: 30rpx 0;
}

.pro_container image {
  width: 656rpx;
  height: 734rpx;
}

.footer_fix {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  background: #fff;
  z-index: 10;
}

.total {
  padding: 18rpx 20rpx;
  background: #f2f5f7;
}

.total_left {
  display: inline-block;
  width: 56%;
}

.total_right {
  display: inline-block;
  width: 35%;
  border-left: 1px solid #e1e4e6;
  font-size: 24rpx;
  color: #8c8c8c;
  padding-left: 30rpx;
}

.money {
  display: inline-block;
  margin-left: 10rpx;
  font-size: 24rpx;
  color: #595959;
  vertical-align: middle;
}

.money text {
  display: inline-block;
  color: #e60012;
  margin-right: 30rpx;
}

.details_btn {
  outline: none;
  width: 100%;
  float: left;
  text-align: center;
  height: 98rpx;
  line-height: 98rpx;
  font-size: 32rpx;
  color: #fff;
  border-radius: 0;
  border: none;
}

.details_btn_no {
  outline: none;
  width: 100%;
  float: left;
  text-align: center;
  height: 98rpx;
  line-height: 98rpx;
  font-size: 32rpx;
  color: #fff;
  border-radius: 0;
  border: none;
}

.details_btn::after, .details_btn_no::after, .details_btn::before,
.details_btn_no::before {
  content: '';
  border: none;
}

.col-e8a40c {
  background: #e8a40c;
}

.col-0068c6 {
  background: #0068c6;
}

.col-cccccc {
  background: #ccc;
}

.swiper-bg {
  width: 100%;
}

.swiper_box {
  width: 100%;
  height: 400rpx;
}

swiper-item image {
  width: 100%;
  height: 100%;
}

.pro_parameter {
  font-size: 28rpx;
  color: #595959;
  display: inline-block;
  width: 590rpx;
  padding: 30rpx 0;
  line-height: 50rpx;
}

.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: scroll;
  z-index: 9000;
  color: #fff;
}

.modal-dialog {
  /* width: 540rpx; *//* overflow: hidden; */
  position: fixed;
  top: 50%;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 9999;
  background: #fff;
  overflow-y: scroll;
  overflow-x: hidden;
  /* margin: -180rpx 105rpx; *//* border-radius: 36rpx; */
}

.txt-bg {
  padding: 0 0 0 20rpx;
}

.stock_title {
  font-size: 34rpx;
  color: #333;
  font-weight: bold;
  padding: 25rpx 0;
}

.stock_line {
  padding: 20rpx 0;
  border-bottom: 1px solid #edf0f2;
}

.stock_line text {
  display: inline-block;
  width: 150rpx;
  font-size: 28rpx;
  color: #595959;
  vertical-align: middle;
  line-height: 30rpx;
}

.stock_inp {
  display: inline-block;
  width: 510rpx;
  font-size: 28rpx;
  color: #595959;
  vertical-align: middle;
  line-height: 30rpx;
}

.apply-btn {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  border-radius: 0;
  height: 99rpx;
  line-height: 99rpx;
  text-align: center;
  color: #fff;
  font-size: 32rpx;
}

.width_box, .width_pic, .width_pic image {
  width: 100% !important;
}

.right_icon {
  display: inline-block;
  width: 12rpx;
  height: 23rpx;
}

.active {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.picker-view {
  width: 100%;
  display: flex;
  z-index: 12;
  background-color: #fff;
  flex-direction: column;
  position: fixed;
  bottom: 0rpx;
  left: 0rpx;
  right: 0;
}

.picker-top {
  padding: 0 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 30rpx;
  color: #8c8c8c;
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 1rpx solid #edf0f2;
  border-top: 1rpx solid #edf0f2;
  box-shadow: rgba(200, 202, 204, 0.1)0 4px 4px;
}

.top-right {
  color: #094;
}

.canvas_box {
  width: 100%;
  height: 1000rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: fixed;
  top: calc(100% - 1000rpx);
  bottom: 80rpx;
  left: 0rpx;
  right: 0rpx;
}

.save_btn {
  width: 100%;
  background-color: #0068c6;
}

/* 设置点点的样式 */
.swiper-bg .wx-swiper-dot {
  width: 10rpx;
  display: inline-flex;
  height: 10rpx;
  margin-left: -4rpx;
  justify-content: space-between;
}

.swiper-bg .wx-swiper-dot::before {
  content: '';
  flex-grow: 1;
  background: #808080;
  border-radius: 8rpx;
  -webkit-border-radius: 8rpx;
  -moz-border-radius: 8rxp;
}

/* 当前点点的样式 */

.swiper-bg .wx-swiper-dot-active::before {
  background: #0068c6;
}

.swiper-bg .wx-swiper-dot.wx-swiper-dot-active {
  width: 30rpx;
}

